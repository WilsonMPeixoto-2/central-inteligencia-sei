{
  "version": 2,
  "buildCommand": "pnpm run build:vercel",
  "devCommand": "npm run dev",
  "installCommand": "pnpm install",
  "framework": null,
  "outputDirectory": "dist/public",
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "api/index.ts": {
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/api/trpc/:path*",
      "destination": "/api"
    }
  ]
}
